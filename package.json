{
  "name": "dist-bot",
  "version": "1.0.0",
  "main": "./build/",
  "license": "MIT",
  "engines": {
    "node": "10.8.0"
  },
  "bin": {
    "del-cli": "./bin/del-cli",
    "nodemon": "./bin/nodemon",
    "concurrently": "./bin/concurrently"
  },
  "scripts": {
    "build": "del-cli build && tsc",
    "nodemon": "nodemon -w build/",
    "watch": "tsc -w",
    "dev": "concurrently --kill-others \"npm run watch\" \"npm run nodemon\" ",
    "start": "npm run build && node ./build/index.js ",
    "populate": "npm run build && node ./build/populate "
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongodb": "^3.1.6",
    "node-telegram-bot-api": "^0.30.0",
    "phantom-html-to-pdf": "^0.5.6",
    "phantomjs-prebuilt": "^2.1.16"
  },
  "devDependencies": {
    "@types/mongodb": "^3.1.10",
    "@types/node": "^10.11.4",
    "del-cli": "^1.1.0",
    "nodemon": "^1.18.4",
    "typescript": "^3.1.1"
  }
}
