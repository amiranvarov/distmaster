{
  "name": "dist-bot",
  "version": "1.0.0",
  "main": "./build/",
  "license": "MIT",
  "bin": {
    "del-cli": "./bin/del-cli",
    "nodemon": "./bin/nodemon",
    "concurrently": "./bin/concurrently"
  },
  "scripts": {
    "build": "del-cli build && tsc",
    "nodemon": "nodemon -w build/",
    "watch": "tsc -w",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run nodemon\" "
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "mongodb": "^3.1.6",
    "node-telegram-bot-api": "^0.30.0"
  },
  "devDependencies": {
    "@types/mongodb": "^3.1.10",
    "@types/node": "^10.11.4",
    "del-cli": "^1.1.0",
    "nodemon": "^1.18.4",
    "typescript": "^3.1.1"
  }
}
